{
  "$schema": "https://schemastore.azurewebsites.net/schemas/json/tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "strictNullChecks": true,
    "lib": ["esnext", "es2015", "dom", "es2016", "es2017"],
    "removeComments": false,
    "noUnusedLocals": true,
    "types": ["cypress", "node"],
    "declaration": true,
    "outDir": "./dist"
  },
  "include": ["./src"]
}
